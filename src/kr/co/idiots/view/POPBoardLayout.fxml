<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="kr.co.idiots.view.POPBoardLayoutController">
   <children>
      <Button id="green" fx:id="btnPost" mnemonicParsing="false" stylesheets="@../../../../../../css/buttonStyle.css" text="글쓰기" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="30.0" />
      <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0" stylesheets="@../../../../../../css/tableViewStyle.css" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="30.0" AnchorPane.rightAnchor="30.0" AnchorPane.topAnchor="30.0">
        <columns>
          <TableColumn fx:id="titleColumn" editable="false" prefWidth="75.0" resizable="false" sortable="false" text="제목" />
          <TableColumn fx:id="authorColumn" editable="false" prefWidth="75.0" resizable="false" sortable="false" text="글쓴이" />
            <TableColumn fx:id="commentCountColumn" editable="false" prefWidth="75.0" resizable="false" sortable="false" text="댓글" />
            <TableColumn fx:id="dateColumn" editable="false" prefWidth="75.0" resizable="false" sortable="false" text="작성일" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
